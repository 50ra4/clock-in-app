{
  "name": "clock-in-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.6.1",
    "date-fns": "2.23.0",
    "date-fns-tz": "1.1.4",
    "firebase": "8.8.0",
    "react": "17.0.2",
    "react-device-detect": "1.17.0",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-scripts": "4.0.3",
    "redux-thunk": "2.3.0",
    "styled-components": "5.3.0",
    "typescript": "4.3.5",
    "web-vitals": "1.1.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.3.5",
    "@storybook/addon-actions": "6.3.5",
    "@storybook/addon-essentials": "6.3.5",
    "@storybook/addon-links": "6.3.5",
    "@storybook/addon-storyshots": "6.3.5",
    "@storybook/node-logger": "6.3.5",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.3.5",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "@types/jest": "26.0.24",
    "@types/node": "12.20.16",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/react-redux": "7.1.18",
    "@types/styled-components": "5.1.11",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "firebase-tools": "9.16.0",
    "husky": "7.0.1",
    "lint-staged": "11.0.1",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "touch": "3.1.0",
    "ts-jest": "26.5.6",
    "ts-node": "10.1.0",
    "typedoc": "0.21.4"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:prettier": "prettier --write '{src,test,tools}/**/*.{ts,tsx}'",
    "lint:eslint": "eslint --fix '{src,test,tools}/**/*.{ts,tsx}'",
    "lint:stylelint": "stylelint 'src/**/*.{ts,tsx}'",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "storybook": "start-storybook -p 6006 -s public",
    "touch-nojeykll": "nodetouch docs/.nojekyll",
    "build-storybook": "rimraf docs/storybook && cpx -C tools/gh-pages/manager-head.html .storybook && build-storybook -s public -o docs/storybook && rimraf .storybook/manager-head.html",
    "build-typedoc": "rimraf docs/typedoc && typedoc --options ./typedoc.json",
    "build-docs": "npm run build-storybook && npm run build-typedoc && npm run touch-nojeykll && cpx -C tools/gh-pages/index.html docs",
    "check-type": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,json}": "prettier --write",
    "*.{ts,tsx}": [
      "prettier --write",
      "stylelint",
      "eslint --cache --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
